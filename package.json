{
  "name": "@tomgutt/azure-ai-search-mcp",
  "version": "1.0.1",
  "description": "Integrate Azure AI Search into agentic workflows via MCP - semantic, hybrid, and text search capabilities",
  "author": "Tom Guttermann",
  "license": "MIT",
  "publisher": "tomgutt",
  "repository": {
    "type": "git",
    "url": "https://github.com/tomgutt/azure-ai-search-mcp.git"
  },
  "keywords": [
    "mcp",
    "agent",
    "autonomous",
    "ai",
    "azure",
    "azure-ai-search",
    "cognitive-search",
    "semantic-search",
    "hybrid-search",
    "vector-search"
  ],
  "scripts": {
    "build": "tsc && node -e \"require('fs').chmodSync('build/index.js', '755')\"",
    "prepare": "npm run build",
    "watch": "tsc --watch",
    "inspector": "npx @modelcontextprotocol/inspector build/index.js",
    "start": "node build/index.js",
    "start:env": "dotenv -e .env -- node build/index.js",
    "test": "node build/test.js"
  },
  "type": "module",
  "devDependencies": {
    "@types/node": "^24.5.0",
    "dotenv": "^17.2.2",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@azure/search-documents": "^12.1.0",
    "@modelcontextprotocol/sdk": "^1.18.0",
    "zod": "^3.25.76"
  },
  "bin": {
    "notion": "./build/index.js"
  },
  "files": [
    "build"
  ]
}
